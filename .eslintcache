tId = pilot\n                    ? pilot.split('/')[5] : null;\n                  return (\n                    <Link to={`/detail/pilots/${pilotId}/`}>\n                      <td key={pilot}>\n                        {pilot}\n                      </td>\n                    </Link>\n                  );\n                })}\n              </tr>\n            ))}\n          </tbody>\n        </Table>\n      </Content>\n      <Pagination>\n        <div>\n          Quantidade:\n          {' '}\n          {total}\n        </div>\n        <PaginationButton>\n          {currentPage > 1 && (\n            <PaginationItem\n              onClick={() => setCurrentPage(currentPage - 1)}\n            >\n              Previous\n            </PaginationItem>\n          )}\n          {pages.map((page) => (\n            <PaginationItem\n              key={page}\n              isSelect={page === currentPage}\n              onClick={() => setCurrentPage(page)}\n            >\n              {page}\n            </PaginationItem>\n          ))}\n          {currentPage < pages.length && (\n            <PaginationItem\n              onClick={() => setCurrentPage(currentPage + 1)}\n            >\n              Next\n            </PaginationItem>\n          )}\n        </PaginationButton>\n\n      </Pagination>\n    </Container>\n  );\n};\n\nexport default Starship;\n","C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\Starship\\styles.js",[],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\People\\index.jsx",["93"],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\components\\Header\\styles.js",[],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\People\\styles.js",[],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\SpecieDetail\\index.jsx",[],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\SpecieDetail\\styles.js",[],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\PilotDetail\\index.jsx",[],"C:\\Users\\cesar\\www\\inatel\\starwars\\src\\pages\\PilotDetail\\styles.